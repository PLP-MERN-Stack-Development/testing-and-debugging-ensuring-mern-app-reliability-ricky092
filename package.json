{
  "name": "mern-testing-assignment",
  "version": "1.0.0",
  "description": "Week 6 Testing and Debugging Assignment",
  "scripts": {
    "test": "jest",
    "test:unit": "jest --selectProjects=client server --testPathPattern=unit",
    "test:integration": "jest --selectProjects=client server --testPathPattern=integration",
    "test:e2e": "cypress run",
    "test:coverage": "jest --coverage",
    "cypress:open": "cypress open",
    "install-all": "npm install && cd client && npm install && cd ../server && npm install"
  },
  "keywords": [
    "mern",
    "testing",
    "jest",
    "cypress"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^13.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0"
  }
}
